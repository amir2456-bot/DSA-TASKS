#include <iostream>
#include <vector>
using namespace std;

int linear_search(const vector<int>& arr, int key) {
    for (size_t i = 0; i < arr.size(); ++i)
        if (arr[i] == key) return i;
    return -1;
}

int main() {
    int n, key;
    cout << "Enter number of elements: ";
    cin >> n;

    vector<int> arr(n);
    cout << "Enter elements:\n";
    for (int i = 0; i < n; ++i)
        cin >> arr[i];

    cout << "Enter number to search: ";
    cin >> key;

    int pos = linear_search(arr, key);

    if (pos != -1) cout << "Element found at index " << pos << endl;
    else cout << "Element not found.\n";

    return 0;
}
